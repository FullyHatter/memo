{
    "env": { "browser": true },
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "semi": ["error", "never", { "beforeStatementContinuationChars": "never" }],
        "no-trailing-spaces": ["error", { "skipBlankLines": true }],
        "comma-dangle": ["error", "never"],
        "no-console": "off",
        "no-use-before-define": "off"
    }
}
